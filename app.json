{
  "expo": {
    "name": "Digital",
    "slug": "todo-mobile",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "./assets/images/Icon-App-1024x1024.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIUserInterfaceStyle": "Light"
      },
      "bundleIdentifier": "com.akyoldanismanlik.digital"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Icon-App-1024x1024.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
